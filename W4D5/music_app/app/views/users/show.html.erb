<section>
  <h1>Welcome <%= current_user.email %></h1>
  <!-- [/[^@]+/] -->
  <% content_for :header do %>
    <form action="<%=session_url%>" method="post">
      <input type="hidden" name="_method" value="delete">
      <%= auth_token %>
      <input class="user_status" type="submit" name="" value="Log Out">
    </form>
  <% end %>
</section>
